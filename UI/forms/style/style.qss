#titleText {
    padding-left:5px;
    color: white;
}

#windowContent QToolButton {
    color: white;
    border: none;
    background-color: #253242;
}

QLabel {
    color: white;
}

QComboBox {
    background-color: #253242;
    color: white;
    min-height: 25px;
    border: 1px solid white;
}

QComboBox::drop-down {
    border: none;
}

QComboBox::down-arrow {
    image: url(":/images/icon_window_minimize.png");
}

QSlider::add-page:vertical {
    background: white;
}

QSlider::sub-page:vertical {
    background: cyan;
}

QScrollArea QWidget {
    background-color: #253242;
}

#divider {
    background-color: #111A23;
    min-width: 100%;
    min-height: 2px;
}
